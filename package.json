{
  "name": "costs-backend2",
  "version": "0.0.0",
  "description": "Node server written in TS.",
  "private": true,
  "scripts": {
    "start": "yarn run watch-ts",
    "build": "yarn run build-ts && yarn run tslint",
    "serve": "node dist/src/server.js",
    "test": "jest --forceExit --coverage",
    "build-ts": "tsc",
    "watch-ts": "nodemon src/server.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/connect-mongo": "^0.0.43",
    "@types/express-session": "^1.15.10",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/mongoose": "^5.5.16",
    "@types/winston": "^2.4.4",
    "async": "^2.6.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "connect-mongo": "^1.3.2",
    "dotenv": "^2.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.17.3",
    "express-flash": "^0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^3.2.1",
    "fbgraph": "^1.4.4",
    "http-status-codes": "^1.3.0",
    "jest": "^19.0.2",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.5",
    "lusca": "^1.5.2",
    "mongoose": "5.0.18",
    "morgan": "^1.9.0",
    "nodemailer": "^2.6.4",
    "passport": "0.3.2",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3",
    "request": "^2.85.0",
    "shelljs": "^0.7.8",
    "supertest": "^3.1.0",
    "ts-jest": "^19.0.8",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.9.2",
    "winston": "^2.4.1",
    "winston-daily-rotate-file": "^3.0.1"
  },
  "devDependencies": {
    "@types/async": "^2.0.48",
    "@types/body-parser": "^1.16.8",
    "@types/dotenv": "^2.0.23",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/jest": "^19.2.2",
    "@types/jquery": "^2.0.49",
    "@types/lodash": "^4.14.106",
    "@types/mongodb": "^2.2.25",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.14.17",
    "@types/nodemailer": "^1.3.33",
    "@types/passport": "^0.3.5",
    "nodemon": "^1.17.5"
  }
}
